FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 300
);

regions
(
	boxToCell
	{
		box (0 -1 2) (9.095 1 2.3);
		fieldValues
		(
			volScalarFieldValue	T 600
		);
	}
	
);

